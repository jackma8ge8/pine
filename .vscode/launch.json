    "version": "0.2.0",
    "configurations": [
        {
            "name": "Connector",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "Server_SystemName": "awesome_pine",
                "Server_ID": "Connector-0",
                "Server_Kind": "connector",
                "Server_Host": "127.0.0.1",
                "Server_Port": 3014,
                "Server_Token": "ksYNdrAo",
                "Server_LogType": "Console",
                "Server_LogLevel": "Debug",
                "Zookeeper_Host": "127.0.0.1",
                "Zookeeper_Port": 2181,
            },
            "args": [],
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Test-client",
            "type": "node",
            "request": "launch",
            "args": [
                "src/index.ts"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}/test-client",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Current TS File",
            "type": "node",
            "request": "launch",
            "args": [
                "${relativeFile}"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
